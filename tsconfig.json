{
  "include": [
    "typing/polymer.js",
    "plugins/*.js"
  ],
  "compilerOptions": {
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowUmdGlobalAccess": true,
    "skipLibCheck": true,
    "target": "es2020",
    "allowJs": true,
    "checkJs": true,
    "noEmit": true,
    "noImplicitAny": true
  }
}
