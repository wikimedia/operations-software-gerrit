{
  "root": true,
  "rules": {
    "arrow-parens": [ "error", "as-needed" ],
    "comma-dangle": "off",
    "computed-property-spacing": "off",
    "indent": [ "error", 2 ],
    "no-cond-assign": "off",
    "no-underscore-dangle": "off",
    "no-useless-escape": "off",
    "space-in-parens": "off"
  }
}
